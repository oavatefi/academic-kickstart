on sysvar sysvar::CH_Nodes::CH__STPDU_EPS_DTQ_Rs_AR2
{
	long res;
	long tDelay;
	switch (@this)
	{
		case (0):
			res = ResetTimingCyclic("DTQ_Rs_AR2");
			res = ResetTimingEvtTrg("DTQ_Rs_AR2");
			res = EnableTimingCyclic("DTQ_Rs_AR2", 1);
			res = EnableTimingEvtTrg("DTQ_Rs_AR2", 1);
			break;
		case (1):
			tDelay = @sysvar::CH_Nodes::CH__STPDU_T_EPS_DTQ_Rs_AR2;
			if (tDelay > 0)
			{
				if (isTimerActive(Timer_EPS_DTQ_Rs_AR2) == 0)
				{
					res = SetTimingCyclic("DTQ_Rs_AR2", 1, tDelay, 1);
					setTimer (Timer_EPS_DTQ_Rs_AR2, tDelay);
					@sysvar::CH_Nodes::CH__STPDU_EPS_DTQ_Rs_AR2 = 2;
				}
			}
			else if (tDelay == 0)
			{
				if (isTimerActive(Timer_EPS_DTQ_Rs_AR2) == 0)
				{
					res = EnableTimingCyclic("DTQ_Rs_AR2", 0);
					res = EnableTimingEvtTrg("DTQ_Rs_AR2", 0);
				}
			}
			break;
		case (2):
			break;
		default:
			Write("error: wrong parameter (%d)", @this);
			stop();
			break;
	}
}

