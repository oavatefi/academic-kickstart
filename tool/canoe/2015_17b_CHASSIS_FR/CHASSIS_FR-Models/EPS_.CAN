void FrILTxPending(char pduName[], long len, byte payload[], long TxAck)
{
	byte crc;
	dword SQC;
	dword MC;
	dword Tgl;
	dword i;

	if (strncmp (pduName, "DTQ_Rs_AR2", strLen(pduName)+strLen("DTQ_Rs_AR2")) == 0)
	{
		if (TxAck == 1)
		{
			if (@sysvar::CH_Nodes::CH_EPSSQC_DTQ_Rs_Pr2DTQ_Rs__AUTO == 0)
			{
				//SQC automatisch ändern
				SQC_VAL_DTQ_Rs_Pr2 = (SQC_VAL_DTQ_Rs_Pr2 + 1) % 16;
			}
			else
			{
				//SQC manuel veränderbar
			}

		}
		else
		{
			if (@sysvar::CH_Nodes::CH_EPSSQC_DTQ_Rs_Pr2DTQ_Rs__AUTO == 0)
			{
				//SQC automatisch ändern
				//SQC für CRC speichern
				CounterValForCRC_DTQ_Rs_Pr2 = SQC_VAL_DTQ_Rs_Pr2;
				//SQC in Byte schreiben
				payload[1] = (payload[1] & ~0x0F) |  SQC_VAL_DTQ_Rs_Pr2;
			}
			else
			{
				//SQC manuel veränderbar
			}

			if (@sysvar::CH_Nodes::CH_EPSCRC_DTQ_Rs_Pr2DTQ_Rs__AUTO == 0)
			{
				//CRC automatisch berechnen
				crc = 0xFF;
				crc = crctableProfTwo[crc ^ payload[1]];
				crc = crctableProfTwo[crc ^ payload[2]];
				crc = crctableProfTwo[crc ^ payload[3]];
				crc = crctableProfTwo[crc ^ payload[4]];
				crc = crctableProfTwo[crc ^ payload[5]];
				crc = crctableProfTwo[crc ^ payload[6]];
				crc = crctableProfTwo[crc ^ payload[7]];
				switch (CounterValForCRC_DTQ_Rs_Pr2)
				{
					case(0) :
						crc = crctableProfTwo[crc ^ 58];
						break;
					case(1) :
						crc = crctableProfTwo[crc ^ 213];
						break;
					case(2) :
						crc = crctableProfTwo[crc ^ 117];
						break;
					case(3) :
						crc = crctableProfTwo[crc ^ 21];
						break;
					case(4) :
						crc = crctableProfTwo[crc ^ 176];
						break;
					case(5) :
						crc = crctableProfTwo[crc ^ 80];
						break;
					case(6) :
						crc = crctableProfTwo[crc ^ 235];
						break;
					case(7) :
						crc = crctableProfTwo[crc ^ 139];
						break;
					case(8) :
						crc = crctableProfTwo[crc ^ 43];
						break;
					case(9) :
						crc = crctableProfTwo[crc ^ 198];
						break;
					case(10) :
						crc = crctableProfTwo[crc ^ 102];
						break;
					case(11) :
						crc = crctableProfTwo[crc ^ 6];
						break;
					case(12) :
						crc = crctableProfTwo[crc ^ 161];
						break;
					case(13) :
						crc = crctableProfTwo[crc ^ 65];
						break;
					case(14) :
						crc = crctableProfTwo[crc ^ 220];
						break;
					case(15) :
						crc = crctableProfTwo[crc ^ 124];
						break;
					default :
//						write("error: wrong parameter (%d)",CounterValForCRC_DTQ_Rs_Pr2); stop();
						break;
				}
				crc = (crc ^ 0xFF);
				payload[0] = crc;
			}
			else
			{
				//CRC manuel veränderbar
			}

			CH_EPSSTA_Rs1_AR2();
			CH_EPSDTQ_Rs_AR2();
		}
	}
	if (strncmp (pduName, "EPS_Stat_Disp_Rq_AR2", strLen(pduName)+strLen("EPS_Stat_Disp_Rq_AR2")) == 0)
	{
		if (TxAck == 1)
		{
			if (@sysvar::CH_Nodes::CH_EPSSQC_EPS_CtrlLmp_Disp__AUTO == 0)
			{
				//SQC automatisch ändern
				SQC_VAL_EPS_CtrlLmp_Disp_Rq_Pr2 = (SQC_VAL_EPS_CtrlLmp_Disp_Rq_Pr2 + 1) % 16;
			}
			else
			{
				//SQC manuel veränderbar
			}

		}
		else
		{
			if (@sysvar::CH_Nodes::CH_EPSSQC_EPS_CtrlLmp_Disp__AUTO == 0)
			{
