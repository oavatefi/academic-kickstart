		putvalue(EnvSendVS6_statEPS_P4U,0);
		putvalue(EnvSendVS6_statESP_P4U,0);
		putvalue(EnvSendVS6_SteeringWheelAngle,0);
		putvalue(EnvSendVS6_TurnIndicatorLeft,0);
		putvalue(EnvSendVS6_TurnIndicatorRight,0);
		putvalue(EnvSendVS6_VehicleSpeed,0);
		putvalue(EnvSendVS6_YawRate,0);
