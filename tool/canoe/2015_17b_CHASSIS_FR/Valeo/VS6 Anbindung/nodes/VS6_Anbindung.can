on message VS6_02
{
    putvalue(EnvPulseCntRearLeft, this.PulseCntRearLeft.phys);
    putvalue(EnvPulseCntRearRight, this.PulseCntRearRight.phys);
    putvalue(EnvPulseCntFrontLeft, this.PulseCntFrontLeft.phys);
    putvalue(EnvPulseCntFrontRight, this.PulseCntFrontRight.phys);
}

on message VS6_01
{
    putvalue(EnvDrivingDir, this.DrivingDir.phys);
    putvalue(EnvReverseGear, this.ReverseGear.phys);
    putvalue(EnvStandstill, this.Standstill.phys);
    putvalue(EnvstatEPS_P4U, this.statEPS_P4U.phys);
    putvalue(EnvstatESP_P4U, this.statESP_P4U.phys);
    putvalue(EnvSteeringWheelAngle, this.Steering.phys);
    putvalue(EnvTurnIndicatorLeft, this.TurnIndicatorLeft.phys);
    putvalue(EnvTurnIndicatorRight, this.TurnIndicatorRight.phys);
    putvalue(EnvVehicleSpeed, this.VehicleSpeed.phys);
    putValue(EnvP4UBtn, this.ButtonP4U.phys); 

    putValue(EnvPDCBtn, this.ButtonUPA.phys);
}

on message VS6_03
{
    putvalue(EnvLateraAccel, this.LateraAccel.phys);
    putvalue(EnvLongituAccel, this.LongituAccel.phys);
    putvalue(EnvYawRate, this.YawRate.phys);
}

