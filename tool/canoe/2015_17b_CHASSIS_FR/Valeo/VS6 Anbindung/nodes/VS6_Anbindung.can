		putvalue(EnvSendVS6_statESP_P4U,1);
		putvalue(EnvSendVS6_SteeringWheelAngle,1);
		putvalue(EnvSendVS6_TurnIndicatorLeft,1);
		putvalue(EnvSendVS6_TurnIndicatorRight,1);
		putvalue(EnvSendVS6_VehicleSpeed,1);
		putvalue(EnvSendVS6_YawRate,1);        
        putvalue(EnvSendVS6_WheelSpeedFrontLeft,1);
        putvalue(EnvSendVS6_WheelSpeedFrontRight,1);
        putvalue(EnvSendVS6_WheelSpeedRearLeft,1);
        putvalue(EnvSendVS6_WheelSpeedRearRight,1);
